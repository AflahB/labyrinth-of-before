<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mongooseofbefore.Labyrinth_of_Before"
          android:versionCode="10"
          android:versionName="2.0">
    <uses-sdk android:minSdkVersion="19"/>
    <application android:label="@string/app_name" android:icon="@drawable/minotaur">
        <activity android:name="com.mongooseofbefore.Labyrinth_of_Before.activities.MainMenuActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.mongooseofbefore.Labyrinth_of_Before.activities.GameActivity"
                    android:screenOrientation="sensorLandscape"/>
        <activity android:name="com.mongooseofbefore.Labyrinth_of_Before.activities.CreditsActivity"/>
    </application>
</manifest>
